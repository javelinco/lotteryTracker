{
  "name": "lotterytracker",
  "version": "1.0.0",
  "description": "Tracks lottery tickets, drawings, winnings and ROI",
  "repository": {
    "type": "git",
    "url": "git://github.com/javelinco/lotteryTracker.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npx jest --watch",
    "start": "ts-node src/index.ts",
    "start:inspect": "node --expose-gc --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts",
    "start:dev": "nodemon --ignore files/ --watch . --exec ts-node src/index.ts",
    "build:dev": "docker build -f Dockerfile.dev -t mmunson/lottery-tracker .",
    "publish:dev": "docker login && docker tag mmunson/lottery-tracker mmunson/lottery-tracker:latest && docker push mmunson/lottery-tracker:latest",
    "compile": "rimraf dist/ && tsc -p .",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:circle": "npm run lint > lintOutput.txt",
    "precommit-message": "echo Running pre-commit checks... && exit 0"
  },
  "author": "Mark Munson",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/hapi__boom": "^7.4.1",
    "@types/hapi__hapi": "^18.2.6",
    "@types/hapi__joi": "^16.0.3",
    "@types/jest": "^24.0.22",
    "@types/node": "^13.1.1",
    "@types/uuid": "^3.4.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jest-runner-groups": "^1.1.1",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.11",
    "@hapi/hapi": "^18.4.1",
    "@hapi/joi": "^16.1.7",
    "dotenv": "^8.2.0",
    "hapi-decorators": "^2.0.0",
    "moment": "^2.24.0",
    "ts-postgres": "^1.1.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  }
}
